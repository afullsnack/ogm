---
import { Astronav, MenuItems } from "astro-navbar";
import Dropdown from "./dropdown.astro";
const { title, children } = Astro.props;
---

<div class="w-full grid items-center justify-end px-5">
  <Astronav>
    <MenuItems>
      <div class="w-full grid items-center justify-start">
        <Dropdown title={title} />
      </div>
    </MenuItems>
  </Astronav>
</div>
